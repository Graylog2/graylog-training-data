      },
      "id": "04ed08dd-a157-4e07-aa78-82303326f0fd",
      "data": {
        "title": {
          "@type": "string",
          "@value": "MQ-17J's Server Messages"
        },
        "description": {
          "@type": "string",
          "@value": "MQ-173 worked really hard to decode these messages."
        },
        "source": {
          "@type": "string",
          "@value": "rule \"MQ-17J's Server Messages\"\nwhen\n  to_string($message.source) == \"multivac\"\nthen\n  let new_message = base64_decode(to_string($message.message));\n  let new_messagea = replace(new_message,\"ug\",\"wn\");\n  let new_messageb = replace(new_messagea,\"igan\",\"alac\");\n  let new_messagec = replace(new_messageb,\"r\",\"line\");\n  set_field(\"message\", new_messagec);\nend"
        }
      },
      "constraints": [
        {
          "type": "server-version",
          "version": ">=5.1.2+d970230"
        }
      ]
    },
